{
  "name": "@chamaeleon/extension-configuration-drawer",
  "private": false,
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Chamaeleon extension configuration drawer",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "check-types": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "@chamaeleon/core": "workspace:*",
    "@chamaeleon/uikit": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "tailwind-config": "workspace:*",
    "tsup": "^7.2.0"
  }
}
