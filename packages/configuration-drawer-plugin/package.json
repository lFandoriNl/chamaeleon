{
  "name": "@chamaeleon/configuration-drawer-plugin",
  "description": "Chameleon plugin for editing block parameters",
  "private": false,
  "version": "0.4.0",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "chameleon",
    "chamaeleon",
    "page builder",
    "editor",
    "change block properties"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "check-types": "tsc --noEmit",
    "test": "echo 'No test specified'"
  },
  "peerDependencies": {
    "@chamaeleon/core": "workspace:~",
    "@chamaeleon/uikit": "workspace:~",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chamaeleon/core": "workspace:~",
    "@chamaeleon/uikit": "workspace:~",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-config": "workspace:*",
    "tsup": "^7.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lFandoriNl/chamaeleon",
    "directory": "packages/configuration-drawer-plugin"
  }
}
