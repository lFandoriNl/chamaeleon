{
  "name": "@chamaeleon/react-editor",
  "description": "Chamaeleon react editor",
  "private": false,
  "version": "0.0.8",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "chameleon",
    "chamaeleon",
    "page builder",
    "editor",
    "react editor"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "check-types": "tsc --noEmit",
    "test": "vitest",
    "clean": "rm -r dist && rm -r .turbo"
  },
  "dependencies": {
    "react": "18.2.0",
    "@chamaeleon/core": "workspace:~",
    "@chamaeleon/uikit": "workspace:~"
  },
  "devDependencies": {
    "@types/react": "18.2.15",
    "tailwind-config": "workspace:*",
    "tsup": "^7.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lFandoriNl/chamaeleon",
    "directory": "packages/react-editor"
  }
}
