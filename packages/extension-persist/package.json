{
  "name": "@chamaeleon/extension-persist",
  "private": false,
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Chamaeleon extension history for undo redo commands",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "module": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "check-types": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@chamaeleon/core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^7.2.0"
  }
}
